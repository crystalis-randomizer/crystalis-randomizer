        $34c82  xx:         xxx
        $34c83  xx xx:      xxx #$xx
        $34c85  xx xx:      xxx #$xx
        $34c87  xx xx:      xxx $xx ; $1b <- %101b cdef, i.e. Axxx or Bxxx
        $34c89  xx xx:      xxx #$xx
