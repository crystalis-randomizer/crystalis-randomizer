;;; --------------------------------
;;; Reads tiles as stored in map data, each corresponding to 4 nametable tiles.
;;; Shift off the low 2 bits from the tile and store in y, $20 gets the low bits.
;;; $10 is (e.g.) $13100.  This is one of 12 64-byte "bitsets" of *pairs* of
;;; bits, selected based on $67 in $3ee42.  By way of example, tile $23 would
;;; get the 6,7 bits from ($10),8, which is looked up into $3ef17,y to translate
;;; to either 0, 55, aa, or ff.
;;; This is looking like attribute table.
LoadMetatileAttribute
        ;; Input:
        ;;   x: The index into $6000 to read the metatile ID
        ;;   ($10): The attribute table for this location
        ;; Output:
        ;;   a: The (2-bit) attribute for the metatile, copied 4 times
        $3eecc  xx xx:      xxx $xx ; save y, which we just read a bit ago
        $3eece  xx xx xx:   xxx $xxxx,x
        $3eed1  xx xx:      xxx $xx
        $3eed3  xx:         xxx
        $3eed4  xx:         xxx
        $3eed5  xx:         xxx
        $3eed6  xx xx:      xxx $xx
        $3eed8  xx xx:      xxx #$xx
        $3eeda  xx xx:      xxx $xx
        $3eedc  xx xx:      xxx ($xx),y
-       $3eede  xx xx:       xxx $xx
        $3eee0  xx xx:       xxx + ; $3eee6
        $3eee2  xx:            xxx
        $3eee3  xx:            xxx
        $3eee4  xx xx:        xxx - ; $3eede
+       $3eee6  xx xx:      xxx #$xx
        $3eee8  xx:         xxx
        $3eee9  xx xx xx:   xxx AttributeQuads,y
        $3eeec  xx xx:      xxx $xx
        $3eeee  xx:         xxx
;;; --------------------------------
TilesetBaseAddresses
        ;; This is a table of 12 addresses (b000 -> 13000), indexed by ($67 & #$3c) >> 1,
        ;; which is stored in $10 for use by the routines here.  Note that this is just
        ;; the high parts of an address - the next lowest part is stored in y in the next
        ;; table, so that ($10),y points to one of 12*8 blocks of 8 bytes.
        ;; STRIP: word=$8000
        $3eeef              .word ($13000) ; $67 = #$80
        $3eef1              .word ($13040) ; $67 = #$84
        $3eef3              .word ($13080) ;       #$88
        $3eef5              .word ($130c0) ;       #$8c
        $3eef7              .word ($13100) ;       #$90
        $3eef9              .word ($13140) ;       #$94
        $3eefb              .word ($13180) ;       #$98
        $3eefd              .word ($131c0) ;       #$9c
        $3eeff              .word ($13200) ;       #$a0
        $3ef01              .word ($13240) ;       #$a4
        $3ef03              .word ($13280) ;       #$a8
        $3ef05              .word ($132c0) ;       #$ac
DataTable_3ef07
        ;; indexed by $36 >> 4 and stored in y. note that the 1-bit is unused;
        ;; it is used to index into 1b and 1d, stored in $12 and $13 (and the
        ;; complement of these in $14).
        $3ef07              .byte $xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx,$xx
AttributeQuads 
        ;; This is for attribute selection.  These bytes apply the 0, 1, 2, or 3
        ;; attribute (respectively) to the four quads of tiles in the block.
        ;; By masking with 3, 30, c, or c0 we can select one or the other.  Or
        ;; in other words, this is 0, 1, 2, and 3 copied 4 times each.
        $3ef17              .byte $xx,$xx,$xx,$xx
AttributeSelector_TopOrBottom
        ;; These are masks for selecting attributes for a single quad of tiles
        ;; within a 4x4 block of tiles that corresponds to a single attribute
        ;; table byte.  $3 and $30 are the (top and bottom, respectively) left
        ;; half of the block, while $c and $c0 are the right half.  That makes
        ;; $3 and $c the top half and $30 and $c0 the bottom half.  If we're
        ;; loading the attributes for two 16x16 metatiles, then we need to
        ;; apply two of these masks to the new tiles and then use the remaining
        ;; bits to mask the previous value of the attribute.  These are tored
        ;; in $12, $13, and the remainder in $14 (see $3ee1b).
        $3ef1b              .byte $xx,$xx
AttributeSelector_LeftOrRight
        $3ef1d              .byte $xx,$xx
