class bd{constructor(){this.flags=new Map}static all(){return[...this.sections]}static flag(a,b){bd.sections.add(this.instance||(this.instance=new this));b=new Ac(a,b);if(!a.startsWith(this.instance.prefix))throw Error("bad flag");this.instance.flags.set(a,b);return b}}bd.sections=new Set;class r extends bd{constructor(){super(...arguments);this.prefix="W";this.name="World"}}
