[])this.flags.delete(Gc.flags.get(a))}else console.error(`Bad flag: ${a}`)}check(a,...b){a=a instanceof Gc?a:Gc.flags.get(a);b.length||b.push(!0);return b.includes(a&&this.flags.get(a)||!1)}get(a){return(a=a instanceof Gc?a:Gc.flags.get(a))&&this.flags.get(a)||!1}preserveUniqueChecks(){return this.check(Zc.PreserveUniqueChecks)}shuffleMimics(){return this.check(Zc.NoShuffleMimics,!1)}buffDeosPendant(){return this.check(ad.BonusItems,!1)}changeGasMaskToHazmatSuit(){return this.check(ad.BonusItems,
