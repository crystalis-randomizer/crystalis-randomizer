p.ELSEIF)){a=!!this.evaluateConst(Vb(e.slice(1),f));continue}else if(p.eq(f,p.ELSE)){a=!0;continue}a&&d.push(e)}this.stream.unshift(...d)}}function Wb(a,b){a=Vb(a,b);return ya.identifier(a)}function Vb(a,b){if(!a.length){if(!b)throw Error("Expected expression");throw Error(`Expected expression: ${p.nameAt(b)}`);}return ya.parseOnly(a)}function Ub(a,b){throw Error(`${p.name(b)} with no ${a}${p.at(b)}`);};class Xb{constructor(){this.stack=[]}next(){for(;this.stack.length;){const [a,b]=this.stack[this.stack.length-1];if(b.length)return b.pop();const c=null===a||void 0===a?void 0:a.next();if(c)return c;this.stack.pop()}}unshift(...a){if(!this.stack.length)throw Error("Cannot unshift after EOF");const b=this.stack[this.stack.length-1][1];for(let c=a.length-1;0<=c;c--)b.push(a[c])}enter(a){const b=[void 0,[]];a&&(b[0]=a);this.stack.push(b);if(100<this.stack.length)throw Error("Stack overflow");}exit(){this.stack.pop()}}
